<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <record id="report_saleorder_document_inh_report" model="ir.ui.view">
        <field name="name">report.saleorder.document.inh.report</field>
        <field name="inherit_id" ref="sale.report_saleorder_document"/>
        <field name="arch" type="xml">

            <!-- Cabecera -->
            <data>
                <xpath expr="//thead/tr/th[@name='th_taxes']" position="replace"/>
                <!-- Cuerpo -->
                <xpath expr="//td[@name='td_taxes']" position="replace"/>
            </data>

        </field>
        <field name="type">qweb</field>
        <field name="priority">999</field>
    </record>
</odoo>
